{
  "segments": {
    "right": [
      {
        "function": "powerline.segments.shell.last_pipe_status",
        "priority": 10
      },
      {
        "function": "powerline_gitstatus.gitstatus",
        "priority": 40
      }
    ]
  },
  "segment_data": {
    "cwd": {
      "args": {
        "dir_shorten_len": 20,
        "dir_limit_depth": 2
      }
    },
    "user": {
      "args": {
        "hide_user": "f"
      }
    }
  }
}
